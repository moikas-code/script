// Recursive fibonacci implementation for benchmarking
fn fibonacci(n: i32) -> i32 {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

// Test with various values
let result1 = fibonacci(10)
let result2 = fibonacci(15)
let result3 = fibonacci(20)

print("Fibonacci(10) = " + result1)
print("Fibonacci(15) = " + result2)
print("Fibonacci(20) = " + result3)